<script async src="//static.getclicky.com/js"></script>


    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <script src="/js/active_page.js" type="text/javascript"></script>
    
   
    <link href="/css/index_style.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Source Sans Pro&display=swap" rel="stylesheet" media="print"
        onload="this.media='all'" />
    <link href="https://fonts.googleapis.com/css?family=Poppins&display=swap" rel="stylesheet" media="print"
        onload="this.media='all'" />
    <link href="https://fonts.googleapis.com/css?family=Open Sans&display=swap" rel="stylesheet" media="print"
        onload="this.media='all'" />
    <title>{{pageTitle}}</title>
    <meta name="description" content="{{postDescription}}">
    <meta property="og:type" content="article">
    <meta property="og:image" content="{{ env.base.img | append cover }}"/>
